  </servlet>
  <servlet-mapping>
    <servlet-name>acceuil</servlet-name>
    <url-pattern>/acceuil</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sign-in</servlet-name>
    <servlet-class>servlets.SignIn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sign-in</servlet-name>
    <url-pattern>/sign-in</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sign-up</servlet-name>
    <servlet-class>servlets.SignUp</servlet-class>
  </servlet>
